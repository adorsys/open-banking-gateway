{
  "name": "fintech-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "openapi-gen": "openapi-generator generate -g typescript-angular -o src/app/api -i ../fintech-api/src/main/resources/static/fintech_api.yml --skip-validate-spec",
    "start": "npm run serve",
    "serve": "ng serve --port=4444 --proxy-config=proxy.conf.json",
    "serve:dev": "ng serve --port=4444 --proxy-config=proxy-conf-dev-backend.js",
    "serve:docker": "ng serve --port=24200 --proxy-config=proxy-docker.conf.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "pretty-quick --staged --pattern 'fintech-examples/fintech-ui/**/*'",
    "format:fix-from-root:pattern": "pretty-quick --pattern 'fintech-examples/fintech-ui/**/*'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1902.3",
    "@angular-devkit/core": "^19.2.3",
    "@angular-devkit/schematics": "^19.2.3",
    "@angular/animations": "^19.2.2",
    "@angular/common": "^19.2.2",
    "@angular/compiler": "^19.2.2",
    "@angular/core": "^19.2.2",
    "@angular/forms": "^19.2.2",
    "@angular/localize": "^19.2.2",
    "@angular/platform-browser": "^19.2.2",
    "@angular/platform-browser-dynamic": "^19.2.2",
    "@angular/router": "^19.2.2",
    "@popperjs/core": "^2.11.8",
    "angular-iban": "^19.0.0",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "5.3.5",
    "font-awesome": "^4.7.0",
    "iban": "0.0.14",
    "jquery": "^3.6.0",
    "ng-http-loader": "^17.0.0",
    "npm": "^10.9.2",
    "popper.js": "^1.16.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.3",
    "@angular-eslint/eslint-plugin": "^19.3.0",
    "@angular-eslint/eslint-plugin-template": "^19.3.0",
    "@angular/cdk": "^19.2.3",
    "@angular/cli": "^19.2.3",
    "@angular/compiler-cli": "^19.2.2",
    "@angular/language-service": "^19.2.2",
    "@cypress/schematic": "^3.0.0",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@ngtools/webpack": "^19.2.3",
    "@openapitools/openapi-generator-cli": "^1.0.18-4.3.1",
    "@schematics/angular": "^19.2.3",
    "@types/jasmine": "^5.1.8",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.30.0",
    "@typescript-eslint/parser": "^8.30.0",
    "angular-eslint": "19.3.0",
    "cypress": "latest",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^4.3.0",
    "jasmine-core": "^5.6.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.1",
    "pretty-quick": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.27.0",
    "uuid": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  }
}
