{
  "name": "consent-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "webdriver-update": "webdriver-manager update",
    "openapi-gen": "openapi-generator generate -g typescript-angular -o src/app/api -i ../opba-consent-rest-api/src/main/resources/static/tpp_consent_api.yml",
    "openapi-auth-gen": "openapi-generator generate -g typescript-angular -o src/app/api-auth -i ../opba-auth-rest-api/src/main/resources/static/tpp_auth_api.yml",
    "start": "ng serve",
    "serve:local": "ng serve --proxy-config=proxy-conf-local-backend.json",
    "serve:dev": "ng serve --proxy-config=proxy-conf-dev-backend.js",
    "serve:docker": "ng serve --port=14200 --proxy-config=proxy-conf-docker-backend.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "e2e": "ng e2e",
    "format:fix": "pretty-quick --staged --pattern 'consent-ui/**/*'",
    "format:fix-from-root:pattern": "pretty-quick --pattern 'consent-ui/**/*'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "0.1902.1",
    "@angular-devkit/core": "19.2.1",
    "@angular/animations": "19.2.1",
    "@angular/cdk": "^19.2.2",
    "@angular/common": "19.2.1",
    "@angular/compiler": "19.2.1",
    "@angular/core": "19.2.1",
    "@angular/forms": "19.2.1",
    "@angular/platform-browser": "19.2.1",
    "@angular/platform-browser-dynamic": "19.2.1",
    "@angular/router": "19.2.1",
    "@ngtools/webpack": "^19.2.1",
    "angular-iban": "^19.0.0",
    "angular2-uuid": "^1.1.1",
    "angularx-qrcode": "^19.0.0",
    "bootstrap": "^5.3.5",
    "iban": "0.0.14",
    "jquery": "^3.6.0",
    "ng-http-loader": "^17.2.0",
    "ng-swagger-gen": "^2.3.1",
    "popper.js": "^1.16.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.0.0",
    "uuidv4": "^6.2.12",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular-eslint/eslint-plugin": "^19.3.0",
    "@angular-eslint/eslint-plugin-template": "^19.3.0",
    "@angular/cli": "19.2.1",
    "@angular/compiler-cli": "19.2.1",
    "@angular/language-service": "19.2.1",
    "@cypress/schematic": "^3.0.0",
    "@eslint/js": "^9.28.0",
    "@openapitools/openapi-generator-cli": "^1.0.18-4.3.1",
    "@types/jasmine": "^5.1.7",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^18.19.80",
    "@typescript-eslint/eslint-plugin": "^8.30.0",
    "@typescript-eslint/parser": "^8.30.0",
    "angular-eslint": "19.3.0",
    "cypress": "latest",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^4.3.0",
    "jasmine-core": "^5.6.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
