  <process id="hbci-list-accounts" isExecutable="true" name="hbci-list-accounts">
        <documentation>HBCI compatible account list</documentation>
        <startEvent flowable:formFieldValidation="true" id="start" name="start">
      <documentation>Starts XS2A account listing flow</documentation>
    </startEvent>
        <endEvent id="end" name="end"/>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciAccountList}" flowable:exclusive="false" flowable:triggerable="false" id="hbciListAccounts" name="Call HBCI account list"/>
        <exclusiveGateway default="hbciPinNeeded" id="hbciCheckPinPresent" name="Check pin is present"/>
        <sequenceFlow id="sid-1c555998-8b35-40ef-b2ee-97e6ac731f23" sourceRef="accountListInitiateConsent" targetRef="hbciCheckPinPresent"/>
        <sequenceFlow id="hbciPinNotNeeded" sourceRef="hbciCheckPinPresent" targetRef="hbciListAccounts">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{hbciConsentInfo.isPasswordPresent(CONTEXT)}]]></conditionExpression>
        </sequenceFlow>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciAskForPin}" flowable:exclusive="true" flowable:triggerable="true" id="hbciAskForPin" name="Ask user to provide PIN. Suspends to wait"/>
        <sequenceFlow id="hbciPinNeeded" sourceRef="hbciCheckPinPresent" targetRef="hbciAskForPin">
            <conditionExpression xsi:type="tFormalExpression"/>
        </sequenceFlow>
        <sequenceFlow id="sid-9796e53b-4891-4072-b34f-8a9587bef409" sourceRef="hbciAskForPin" targetRef="hbciCheckPinPresent"/>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciAskForTanChallenge}" flowable:exclusive="false" flowable:triggerable="true" id="askUserForTAN" name="Ask user to provide TAN. Suspends to wait"/>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciAccountListWithTan}" flowable:exclusive="false" flowable:triggerable="false" id="provideAccountListResult" name="Provide account list result">
            <documentation>Call HBCI with TAN</documentation>
        </serviceTask>
        <sequenceFlow id="sid-56d50940-9b81-4d42-b75d-dbe9a18fa7bd" sourceRef="hbciListAccounts" targetRef="hbciCheckTanNeeded"/>
        <sequenceFlow id="hbciTanNeeded" sourceRef="hbciCheckTanNeeded" targetRef="askUserForTAN">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{hbciConsentInfo.isTanChallengeRequired(CONTEXT)}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="sid-2586ffcd-3f7d-48df-a50f-2e2b08de4c7a" sourceRef="askUserForTAN" targetRef="provideAccountListResult"/>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciAisConsentInitiate}" flowable:exclusive="false" flowable:triggerable="false" id="accountListInitiateConsent" name="Initiate consent for account list">
            <documentation>Ensures that user opens UI landing page after login</documentation>
        </serviceTask>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciStoreAccountListToCache}" flowable:exclusive="false" flowable:triggerable="false" id="cacheAccountList" name="Cache account list"/>
        <sequenceFlow id="sid-fce0138c-0696-4db7-9920-e32dc4accf2e" sourceRef="provideAccountListResult" targetRef="cacheAccountList"/>
        <exclusiveGateway default="sid-05fe18f0-a41f-4112-ac7b-f15e6f9cbee4" id="cachedAccountListChecker" name="Cached account list present?"/>
        <sequenceFlow id="sid-8528c97f-afbd-45df-97dc-527e05ef6d9a" sourceRef="readAccountListFromCache" targetRef="cachedAccountListChecker"/>
        <sequenceFlow id="sid-3f0ca2d6-371a-4c18-bdde-6fd5b6981862" sourceRef="cachedAccountListChecker" targetRef="accountListInitiateConsent">
            <conditionExpression xsi:type="tFormalExpression">#{hbciConsentInfo.isCachedAccountListMissing(CONTEXT)}</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="hbciTanNotNeeded" sourceRef="hbciCheckTanNeeded" targetRef="cacheAccountList">
            <conditionExpression xsi:type="tFormalExpression"/>
        </sequenceFlow>
        <exclusiveGateway default="hbciTanNotNeeded" id="hbciCheckTanNeeded" name="Check if TAN challenge is needed"/>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciReadAccountListFromCache}" flowable:exclusive="false" flowable:triggerable="false" id="readAccountListFromCache" name="Read account list from cache"/>
        <sequenceFlow id="sid-05fe18f0-a41f-4112-ac7b-f15e6f9cbee4" sourceRef="cachedAccountListChecker" targetRef="publishAccountListResult"/>
        <sequenceFlow id="sid-a706977a-d880-47e9-aee2-d1d7de72ae8f" sourceRef="cacheAccountList" targetRef="reportConsentAuthorized"/>
        <sequenceFlow id="sid-48d18d7a-caad-4c63-bf4a-c6298716dafc" sourceRef="start" targetRef="readAccountListFromCache"/>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciPublishAccountListResult}" flowable:exclusive="false" flowable:triggerable="false" id="publishAccountListResult" name="Publish account list result"/>
        <sequenceFlow id="sid-63b1b1ba-b99f-4a82-b22a-3082075f25e4" sourceRef="publishAccountListResult" targetRef="end"/>
        <serviceTask flowable:async="true" flowable:delegateExpression="${hbciReportToFintechConsentAuthorized}" flowable:exclusive="false" flowable:triggerable="false" id="reportConsentAuthorized" name="Report to FinTech that consent was authorized"/>
        <sequenceFlow id="sid-5b1e9eba-c5ab-4baf-9472-b7f269f96a72" sourceRef="reportConsentAuthorized" targetRef="end"/>
    </process>
