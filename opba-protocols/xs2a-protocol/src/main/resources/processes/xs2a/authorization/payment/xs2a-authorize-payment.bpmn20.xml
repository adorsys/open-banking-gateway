      <sequenceFlow id="startAuthorizationChecking" sourceRef="decoupledSubProcessStart" targetRef="checkIfDecoupledScaFinalized" name="Start authorization checking"/>
      <sequenceFlow id="sid-ed27275e-353c-4cf7-92d6-0d017cd2d8f0" sourceRef="checkIfDecoupledScaFinalized" targetRef="isAuthorizationSucceded"/>
      <endEvent id="subprocessEnd" name="End of decoupled Sca status checking subprocess"/>
      <sequenceFlow id="finishSubprocess" sourceRef="isAuthorizationSucceded" targetRef="subprocessEnd" name="Successful proceeding to the subprocess end">
        <conditionExpression xsi:type="tFormalExpression"/>
      </sequenceFlow>
      <intermediateCatchEvent id="waitAuthorizationConfirmation" name="Waits until authorization is finished by PSU">
        <timerEventDefinition>
          <timeDuration>#{flowableProperties.jobTimeout.pollInterval}</timeDuration>
        </timerEventDefinition>
      </intermediateCatchEvent>
      <sequenceFlow id="startTimer" sourceRef="isAuthorizationSucceded" targetRef="waitAuthorizationConfirmation" name="Waits for user to authorize in 3rd party using decoupled approach">
        <conditionExpression xsi:type="tFormalExpression">#{!xs2aConsentInfo.isDecoupledScaFinalizedByPSU(CONTEXT)}</conditionExpression>
      </sequenceFlow>
      <sequenceFlow id="restartAuthorizationCheck" sourceRef="waitAuthorizationConfirmation" targetRef="checkIfDecoupledScaFinalized" name="Starts authorization check after some delay"/>
      <exclusiveGateway id="isAuthorizationSucceded" name="Authirization finalised?" default="finishSubprocess"/>
    </subProcess>
    <boundaryEvent id="subprocessTimeLimit" attachedToRef="decoupledScaStatusChecking" name="Decoupled authorization time limit (timeout)">
      <timerEventDefinition>
        <timeDuration>#{flowableProperties.jobTimeout.timeout}</timeDuration>
      </timerEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="decoupledSubprocessFailure" sourceRef="subprocessTimeLimit" targetRef="sid-xs2aReportToFintechPaymentDenied" name="Decoupled authorithation failed by timeout"/>
    <sequenceFlow id="decoupledScaSelected" sourceRef="isDecoupledScaSelected" targetRef="decoupledScaStatusChecking">
      <conditionExpression xsi:type="tFormalExpression">#{xs2aConsentInfo.isDecoupledScaSelected(CONTEXT)}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="decoupledSubprocessSuccess" sourceRef="decoupledScaStatusChecking" targetRef="isScaFinalized" name="Decoupled authorithation was successful"/>
